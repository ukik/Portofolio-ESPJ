{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-cors": "^0.11.2",
        "chumper/zipper": "^1.0",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^2.1",
        "riskihajar/terbilang": "^1.2",
        "spatie/db-dumper": "^2.10",
        "spatie/laravel-activitylog": "^2.8",
        "tymon/jwt-auth": "^0.5.12"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "app/E_SPJ/Authenticate",
            "app/E_SPJ/Authenticate/Passport",
            "app/E_SPJ/Authenticate/JWT",

            "app/E_SPJ/Static",

            "app/E_SPJ/Middleware",
            "app/E_SPJ/Middleware/JWT",
            "app/E_SPJ/Middleware/Signiture",

            "app/E_SPJ/Controller",
            "app/E_SPJ/Controller/Entry",
            "app/E_SPJ/Controller/Entry/DataDasar",
            "app/E_SPJ/Controller/Entry/DataDasar/Lokasi",
            "app/E_SPJ/Controller/Entry/Lampiran",
            "app/E_SPJ/Controller/Entry/Lampiran/Honor",
            "app/E_SPJ/Controller/Entry/Lampiran/Perjadin",
            "app/E_SPJ/Controller/Entry/Lampiran/Spk",
            "app/E_SPJ/Controller/Entry/Login",
            "app/E_SPJ/Controller/Entry/Pengaturan",
            "app/E_SPJ/Controller/Entry/Permohonan",
            "app/E_SPJ/Controller/Entry/Spj",
            "app/E_SPJ/Controller/Entry/Standar",
            "app/E_SPJ/Controller/Entry/Standar/Perjadin",
            "app/E_SPJ/Controller/Entry/Standar/Perjadin/Harian",
            "app/E_SPJ/Controller/Entry/Standar/Perjadin/Hotel",
            "app/E_SPJ/Controller/Entry/Verifikasi",
            "app/E_SPJ/Controller/Entry/Rekap",

            "app/E_SPJ/Model/Attribute",
            "app/E_SPJ/Model/Attribute/_Common",
            "app/E_SPJ/Model/Attribute/_Common/Accessor",
            "app/E_SPJ/Model/Attribute/_Common/Miscellaneous",
            "app/E_SPJ/Model/Attribute/_Common/Override",

            "app/E_SPJ/Model/Attribute/DataDasar",
            "app/E_SPJ/Model/Attribute/DataDasar/Pegawai",
            "app/E_SPJ/Model/Attribute/Lampiran",
            "app/E_SPJ/Model/Attribute/Lampiran/Honor",
            "app/E_SPJ/Model/Attribute/Lampiran/Perjadin",
            "app/E_SPJ/Model/Attribute/Lampiran/Spk",
            "app/E_SPJ/Model/Attribute/Permohonan",
            "app/E_SPJ/Model/Attribute/Permohonan/GantiUang",
            "app/E_SPJ/Model/Attribute/Permohonan/TambahUang",
            "app/E_SPJ/Model/Attribute/Spj",
            "app/E_SPJ/Model/Attribute/Standar",
            "app/E_SPJ/Model/Attribute/Standar/Perjadin",
            
            "app/E_SPJ/Model/Entry/Get",
            "app/E_SPJ/Model/Entry/Get/DataDasar",
            "app/E_SPJ/Model/Entry/Get/DataDasar/Lokasi",
            "app/E_SPJ/Model/Entry/Get/DataDasar/Pegawai",
            "app/E_SPJ/Model/Entry/Get/Lampiran",
            "app/E_SPJ/Model/Entry/Get/Lampiran/Honor",
            "app/E_SPJ/Model/Entry/Get/Lampiran/Perjadin",
            "app/E_SPJ/Model/Entry/Get/Lampiran/Perubahan",
            "app/E_SPJ/Model/Entry/Get/Lampiran/Spk",
            "app/E_SPJ/Model/Entry/Get/Standar",
            "app/E_SPJ/Model/Entry/Get/Standar/Perjadin/Harian",
            "app/E_SPJ/Model/Entry/Get/Standar/Perjadin/Hotel",
            "app/E_SPJ/Model/Entry/Get/Rekap",

            "app/E_SPJ/Model/Entry/Post",
            "app/E_SPJ/Model/Entry/Post/DataDasar",
            "app/E_SPJ/Model/Entry/Post/DataDasar/Lokasi",
            "app/E_SPJ/Model/Entry/Post/DataDasar/Pegawai",
            "app/E_SPJ/Model/Entry/Post/Lampiran",
            "app/E_SPJ/Model/Entry/Post/Lampiran/Honor",
            "app/E_SPJ/Model/Entry/Post/Lampiran/Perjadin",
            "app/E_SPJ/Model/Entry/Post/Lampiran/Perubahan",
            "app/E_SPJ/Model/Entry/Post/Lampiran/Spk",
            "app/E_SPJ/Model/Entry/Post/Standar",
            "app/E_SPJ/Model/Entry/Post/Standar/Perjadin/Harian",
            "app/E_SPJ/Model/Entry/Post/Standar/Perjadin/Hotel",

            "app/E_SPJ/Model/Scope",
            "app/E_SPJ/Model/Scope/_Boot",
            "app/E_SPJ/Model/Scope/_Common",
            "app/E_SPJ/Model/Scope/_Common/Paginate",
            "app/E_SPJ/Model/Scope/_Common/Query",

            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/E_SPJ/Library/Callback/Response.php",

            "app/E_SPJ/Library/Default/Cache.php",
            "app/E_SPJ/Library/Default/Crypt.php",
            "app/E_SPJ/Library/Default/Date.php",
            "app/E_SPJ/Library/Default/Field.php",
            "app/E_SPJ/Library/Default/JWT.php",
            "app/E_SPJ/Library/Default/Paginate.php",
            "app/E_SPJ/Library/Default/Request.php",
            "app/E_SPJ/Library/Default/Security.php",
            "app/E_SPJ/Library/Default/Sql.php",
            "app/E_SPJ/Library/Default/String.php",
            "app/E_SPJ/Library/Default/Utility.php",

            "app/E_SPJ/Library/Deprecated/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
